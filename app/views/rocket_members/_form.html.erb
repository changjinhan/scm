<%= f.label :rocket_id, "기수" %>
<%= f.select :rocket_id, options_from_collection_for_select(Rocket.all.order('id DESC'), :id, :unit), {}, class: "form-control" %>
<%= f.label :email, "Instagram" %>
<%= f.text_field :email, requied: true, class: "form-control" %>
<%= f.label :name, "이름" %>
<%= f.text_field :name, requied: true, class: "form-control" %>
<%= f.label :identity, "신분" %>
<%= f.text_field :identity, class: "form-control" %>
<%= f.label :follower, "팔로워" %>
<%= f.number_field :follower, class: "form-control" %>
<%= f.label :group, "그룹" %>
<%= f.number_field :group, class: "form-control" %>
<%= f.label :homepage, "홈페이지" %>
<%= f.text_field :homepage, class: "form-control" %>
<%= f.label :product_id, "관심제품" %>
<%= f.select :product_id, options_from_collection_for_select(Product.all, :id, :name), {}, class: "form-control" %>
<%= f.label :application, "지원서", class: "label-top" %>
<%= f.text_area :application, class: "form-control application-text-area" %>
